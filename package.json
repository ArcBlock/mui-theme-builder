{
  "name": "@blocklet/theme-builder",
  "private": true,
  "description": "A comprehensive tool to customize themes for Material-UI",
  "version": "0.1.23",
  "keywords": [
    "MUI",
    "MUI v5",
    "Material-UI",
    "MUI Theme Creator"
  ],
  "contributors": [
    "ArcBlock (https://github.com/ArcBlock)"
  ],
  "files": [
    "dist",
    "public",
    "screenshots",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "dev": "vite --host",
    "serve": "vite preview",
    "lint": "eslint src --ext .mjs,.js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "clean": "node scripts/build-clean.mjs",
    "build": "./extractMuiVersion.sh && vite build && node publish.js",
    "prepare": "npx simple-git-hooks",
    "bump-version": "zx --quiet scripts/bump-version.mjs",
    "fetch-fonts": "node scripts/fetch-google-fonts.mjs",
    "update:deps": "taze -r -w && pnpm install && pnpm dedupe && pnpm build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage"
  },
  "lint-staged": {
    "*.{mjs,js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint"
    ],
    "*.{css,less,scss,json,graphql}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@abtnode/constant": "^1.16.44",
    "@arcblock/did-connect": "^2.13.66",
    "@arcblock/icons": "^2.13.66",
    "@arcblock/ux": "^2.13.66",
    "@babel/eslint-parser": "^7.27.5",
    "@blocklet/theme": "^2.13.66",
    "@blocklet/ui-react": "^2.13.66",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@iconify/react": "^5.2.1",
    "@mui/icons-material": "^5.17.1",
    "@mui/lab": "5.0.0-alpha.176",
    "@mui/material": "^5.17.1",
    "@mui/styles": "^5.17.1",
    "@tanstack/react-virtual": "^3.13.11",
    "@types/recharts": "^1.8.29",
    "ahooks": "^3.8.5",
    "axios": "^1.9.0",
    "dot-prop-immutable": "^2.1.1",
    "flat": "^5.0.2",
    "is-url": "^1.2.4",
    "json5": "^2.2.3",
    "lodash": "^4.17.21",
    "mdi-material-ui": "^7.9.4",
    "moment": "^2.30.1",
    "monaco-editor": "^0.20.0",
    "nanoid": "^5.1.5",
    "prettier": "^3.5.3",
    "react": "~18.2.0",
    "react-color": "^2.19.3",
    "react-colorful": "^5.6.1",
    "react-dom": "~18.2.0",
    "react-redux": "^7.2.9",
    "react-router-dom": "^7.6.2",
    "recharts": "^2.15.3",
    "redux": "^4.2.1",
    "redux-thunk": "^2.4.2",
    "reselect": "^4.1.8",
    "ufo": "^1.6.1",
    "webfontloader": "^1.6.28",
    "xhr-mock": "^2.5.1",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@arcblock/eslint-config-ts": "^0.3.3",
    "@testing-library/react": "^14.3.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/codemirror": "^0.0.97",
    "@types/dot-prop-immutable": "^1.5.4",
    "@types/flat": "^5.0.5",
    "@types/is-url": "^1.2.32",
    "@types/lodash": "^4.17.17",
    "@types/react-color": "^3.0.13",
    "@types/react-redux": "^7.1.34",
    "@types/webfontloader": "^1.6.38",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "~7.16.1",
    "@vitejs/plugin-react": "^4.5.2",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.7",
    "bumpp": "^9.11.1",
    "eslint": "^8.57.1",
    "jsdom": "^22.1.0",
    "lint-staged": "^15.5.2",
    "rimraf": "^5.0.10",
    "simple-git-hooks": "^2.13.0",
    "taze": "^19.1.0",
    "typescript": "~5.5.4",
    "vite": "^5.4.19",
    "vite-plugin-blocklet": "^0.9.33",
    "vite-plugin-monaco-editor": "^1.1.0",
    "vite-plugin-svgr": "^4.3.0",
    "vitest": "^0.34.6",
    "zx": "^8.5.5"
  },
  "license": "0BSD",
  "repository": {
    "type": "git",
    "url": "https://github.com/ArcBlock/mui-theme-builder"
  },
  "bugs": {
    "url": "https://github.com/ArcBlock/mui-theme-builder/issues"
  }
}
